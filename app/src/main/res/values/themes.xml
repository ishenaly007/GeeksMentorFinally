<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RetrofitTest" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.RetrofitTest.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RetrofitTest.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RetrofitTest.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <!-- Custom app themes -->
    <style name="btn_white_and_black">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:backgroundTint">#262523</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/sf_medium</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="btn_yellow_and_black">
        <item name="android:textColor">#262523</item>
        <item name="android:backgroundTint">#FFE600</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/sf_medium</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="et_white_and_black">
        <item name="android:textColorHint">#9E9E9E</item>
        <item name="android:textColor">#9E9E9E</item>
        <item name="android:background">@drawable/et_background</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/sf_regular</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="backgroundTint">#F5F6F6</item>
    </style>

    <!-- Base application theme. -->
    <style name="Base.Theme.RetrofitTest" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->

    </style>
</resources>